        <attr name="rll_normalSolidColor" format="color" />
        <attr name="rll_leftTopRadius" format="dimension" />
        <attr name="rll_rightTopRadius" format="dimension" />
        <attr name="rll_leftBottomRadius" format="dimension" />
        <attr name="rll_rightBottomRadius" format="dimension" />
        <attr name="rll_strokeColor" format="color" />
